{
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.server.launchMode": "Standard",

  "java.format.settings.url": ".vscode/eclipse-java-google-style.xml",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true
  },
  "cSpell.words": [
    "CANID",
    "deadband",
    "navx"
  ],
  "[java]": {
    "editor.tabSize": 2
  }
}
